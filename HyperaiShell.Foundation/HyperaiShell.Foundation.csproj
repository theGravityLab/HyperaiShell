    <PackageId>HyperaiShell.Foundation</PackageId>
    <Version>2020.9.19</Version>
    <Authors>GravityLab</Authors>
    <Company>Unicore</Company>
    <PackageDescription>Basics for HyperShell plugins</PackageDescription>
    <RepositoryUrl>https://github.com/theGravityLab/HyperaiShell.Foundation</RepositoryUrl>
    <Product>HyperaiShell</Product>
